{
  "name": "salad-days",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.34.0",
    "animate.css": "^3.7.2",
    "http-proxy-middleware": "^0.19.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "randomcolor": "^0.5.4",
    "react": "^16.14.0",
    "react-awesome-slider": "^4.1.0",
    "react-dom": "^16.14.0",
    "react-icons": "^3.10.0",
    "react-image-gallery": "^1.0.8",
    "react-modal": "^3.11.2",
    "react-moment": "^0.9.2",
    "react-redux": "^7.2.0",
    "react-responsive-carousel": "^3.2.9",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-form": "^8.2.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "shopify-buy": "^2.11.0",
    "shopify-buy-with-tags": "^2.12.0",
    "storm-react-diagrams": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "identity-obj-proxy": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "react-scripts": "2.1.8",
    "redux-mock-store": "^1.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "always",
    "tabWidth": 2
  },
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/jestsetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/assetsTransformer.js",
      "\\.(scss|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>[/\\\\]node_modules[/\\\\].+\\.(ts|tsx|js|jsx)$"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
